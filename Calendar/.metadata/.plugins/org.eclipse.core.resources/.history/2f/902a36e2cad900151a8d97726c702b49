package br.edu.ifpb.calendarwars.entities;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;

@Entity
public class CalendarGeral implements Calendar{
	@Id
	@GeneratedValue(strategy=GenerationType.IDENTITY)
	private Long id;
	@Column
	private String type;
	@Column
	private String Descricao;	
	
	@ManyToOne
	private CalendarGeral substituto;
	
	
	public CalendarGeral(){}
	
	public CalendarGeral(String type, String descricao, String date) {		
		this.type = type;
		Descricao = descricao;		
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getType() {
		return type;
	}

	public String getDescricao() {
		return Descricao;
	}

	public void setDescricao(String descricao) {
		Descricao = descricao;
	}

}
